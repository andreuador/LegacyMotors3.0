<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Legacy Motors{% endblock %}</title>
    <link rel="icon" href="{{ asset('images/logo_blanc.png') }}">
    <link href="{{ asset('css/styles.css') }}" rel="stylesheet">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body class="p-0">
{% include '_header_front.html.twig' %}
{# FLASH MESAGGE #}
{% for label, messages in app.flashes %}
    {% for message in messages %}
        <div class="alert alert-{{ label }} alert-dismissible fade show" role="alert">
            {{ message }}
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
            </button>
        </div>
    {% endfor %}
{% endfor %}
<main role="main">
    {% block body %}
    {% endblock %}
</main>
{% include '_footer_front.html.twig' %}
</body>
</html>

