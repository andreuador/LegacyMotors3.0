<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Legacy Motors{% endblock %}</title>
    <link rel="icon" href="{{ asset('images/logo7.png') }}">
    <link href="{{ asset('css/styles.css') }}" rel="stylesheet">
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% include '_header_back.html.twig' %}
<main class="container-fluid">
        <div class="row">
            {#{% include '_aside_back.html.twig' %}#}
            <aside class="col-sm-2" {{ vue_component('AsideBack_component') }}></aside>
            <section class="col-lg-10 col-md-12">
                {% block body %}
                {% endblock %}
            </section>
        </div>
</main>
{% include '_footer_front.html.twig' %}
</body>
</html>
